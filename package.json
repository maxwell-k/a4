{
  "name": "@maxwell-k/a4",
  "version": "0.2.2",
  "type": "module",
  "description": "Create a PDF with puppeteer",
  "main": "a4.ts",
  "scripts": {
    "test": "NODE_OPTIONS='--experimental-transform-types --disable-warning=ExperimentalWarning' curl --silent --remote-name http://example.org/index.html && ./a4.ts index.html && ./is-a4.ts index.pdf",
    "check": "deno fmt --check && deno lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxwell-k/a4.git"
  },
  "keywords": [
    "pdf",
    "a4",
    "papersize",
    "puppeteer"
  ],
  "author": "Keith Maxwell <keith.maxwell@gmail.com>",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/maxwell-k/a4/issues"
  },
  "homepage": "https://github.com/maxwell-k/a4#readme",
  "bin": "a4.ts",
  "dependencies": {
    "commander": "^14.0.0",
    "puppeteer-core": "^24.0.0"
  },
  "devDependencies": {
    "pdfjs-dist": "5.4.530"
  },
  "engines": {
    "node": ">=22"
  }
}
